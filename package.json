{
  "name": "next-template",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint:fix": "next lint --fix",
    "lint": "next lint",
    "prettier:fix": "prettier --write \"src/**/*.{ts,tsx,scss}\"",
    "prettier:check": "prettier --check \"src/**/*.{ts,tsx,scss}\"",
    "format": "yarn lint:fix && yarn type:check",
    "ci:lint": "yarn lint && yarn type:check",
    "type:check": "tsc --noEmit",
    "prepare": "husky"
  },
  "dependencies": {
    "jotai": "^2.9.3",
    "jotai-effect": "^1.0.3",
    "lodash": "^4.17.21",
    "next": "14.2.8",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@types/lodash": "^4.17.7",
    "@types/node": "^22.5.4",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^8.4.0",
    "@typescript-eslint/parser": "^8.4.0",
    "eslint": "8.57.0",
    "eslint-config-next": "14.2.8",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-tailwindcss": "^3.17.4",
    "husky": "^9.1.5",
    "postcss": "^8.4.45",
    "prettier": "^3.3.3",
    "sass": "^1.78.0",
    "tailwindcss": "^3.4.10",
    "typescript": "5.5.4"
  },
  "packageManager": "yarn@4.3.1"
}
